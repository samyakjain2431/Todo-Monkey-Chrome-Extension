{
    "manifest_version": 3,
    "name": "Todo Monkey ğŸ’",
    "description": "This monkey will help you create todo list",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "./img/img1.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "permissions" : ["activeTab", "storage", "scripting"]
  }